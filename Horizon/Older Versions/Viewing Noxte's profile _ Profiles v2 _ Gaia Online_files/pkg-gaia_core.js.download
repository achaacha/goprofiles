/**
 * Detect Flash version at beginning of session and store the version found in a cookie
 * Domains supported:
 *  - gaiaonline.com
 *  - gaiaonlinehelp.com
 * Any other domains will throw an error (which is internally caught and swallowed) but will not write the cookie.
 * @require yahoo cookie gaialoader gaiaswfobject
 * @author CarbonPhyber
 */
 
(function () {
    // create a GaiaLoader and run it
    var Y = YAHOO,
        YU = Y.util,
        flashVersionStored = YU.Cookie.get("_clt");
    if(null === flashVersionStored) {
        YU.Get.script(["//" + GAIA_config("jscompiler_server") + "/src/js/flash/swfobject.js"], {
            onSuccess: function () {
                // if the cookie didn't exist, load gaiaswfobject and detect the Flash version + write to cookie
                var playerVersion = swfobject.getFlashPlayerVersion(), // returns a JavaScript object
                    output = swfobject.hasFlashPlayerVersion("1.0") ? [playerVersion.major, ".", playerVersion.minor, ".", playerVersion.release].join("") : "-1", // possible values: "-1" (no Flash detected) or "major.minor.revision" (where major/minor/revision are integers)
                    hostname;
                // catch+swallow exceptions just in case
                try {
                    superdomain = location.host.replace(/^.*\.(\w+\.\w+)$/, "$1"); // grab the superdomain (i.e. gaiaonline.com or gaiaonlinehelp.com) -- this regex will grab the last two "words" of the URL's "host"
                    if("gaiaonline.com" === superdomain || "gaiaonlinehelp.com" === superdomain) { // we only care about these superdomains
                        YU.Cookie.set("_clt", output, {
                            path: "/",              //all pages
                            domain: superdomain     // all domains in/under this superdomain
                        });
                    }
                } catch (err) {
                    // ;
                }
            }
            // Nothing to do if the Cookie has already been set
        });
    }
})();

