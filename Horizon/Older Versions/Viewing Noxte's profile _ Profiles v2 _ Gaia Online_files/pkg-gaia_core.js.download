        // return "//"+GAIA_config("JSCOMPILER_SERVER")+"/src/_/"+GAIA_config("CACHE_VALUE")+"/css/cashshop/" + name;
        return "//"+GAIA_config("GRAPHICS_SERVER")+"/src/css/cashshop/" + name + "?"+GAIA_config("CACHE_VALUE");
    };

    var server = GAIA_config("jscompiler_server");
    if (window.location.search.match(/nocdn=true/)) {
      server = GAIA_config('main_server');
    }

    var package_base = "//" + server + "/src/_/" + GAIA_config("cache_value") + "/";
    var queued_show = false;
    var fetching = false;
    
    var urls = {
        css_ie7: [createCashshopCSSPath("cashshop_ie7.css")]
    };

    var getPackages = function() {
      /* get the CSS */
      YAHOO.util.Get.css(package_base + "pkg-cashshop.css", {
          onSuccess: function() {
            if (YAHOO.env.ua.ie == 7) {
              YAHOO.util.Get.css(urls.css_ie7);
            }
          }
      });

      /* get the JS package */
      YAHOO.util.Get.script(package_base + "pkg-cashshop.js", {
        onSuccess: function() {
          if (queued_show) {
            YAHOO.gaia.app.CashShop.SenderApplication.show(queued_show);
            queued_show = false;
          }
        }
      });
    };

    YAHOO.namespace("gaia.app.CashShop.SenderApplication");
    YAHOO.namespace("gaia.app.CashShop.Modules");
    
    // redefine the sender app's SHOW until we actually have it loaded
    YAHOO.gaia.app.CashShop.SenderApplication = {
        show: function(oConfig) {
            queued_show = oConfig;
            if (!fetching) {
              fetching = true;

              getPackages();
	    }
        }
